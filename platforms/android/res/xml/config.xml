<?xml version='1.0' encoding='utf-8'?>
<widget id="com.avnet.todo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>ToDo</name>
    <description>
       To Do application to speed your activity process
    </description>
    <author email="im.manikanta@gmail.com" href="http://cordova.io">
        Mani
    </author>
    <icon density="mdpi" height="57" platform="android" src="www/img/todo.png" width="57" />
    <content src="index.html" />
    <access origin="*" />
    <preference name="splashscreen" value="splash" />
    <preference name="splashScreenDelay" value="1000" />
</widget>
